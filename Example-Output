def fibonacci(n):
    if n <= 1:
        return n
    else:
        return fibonacci(n-1) + fibonacci(n-2)

if __name__ == "__main__":
    import sys
    argv = sys.argv[1:]
    input_data = int(argv[0]) if len(argv) > 0 else None
    
    result = fibonacci(input_data)
    print(result)
